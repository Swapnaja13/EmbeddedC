/******************************************************************************

Execute the line and find the output 

*******************************************************************************/

#include <stdio.h>

int main()
{

    int a=2,b=1,x;
    x=(a++&&(!b)&&b||++a);  // 0&&x -> x will never evalute  
                            // 1||x -> x will never evalute
    printf("x=%d",x);
}
