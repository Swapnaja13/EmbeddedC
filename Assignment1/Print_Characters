#include <stdio.h>

int main()
{
    char ch1,ch2,ch3;
    ch1=65;
    ch2='b';
    ch3='3';
    printf("ch1=%c\n",ch1);     // prints equivalent character
    printf("ch1=%c\n",ch2);
    printf("ch1=%c\n",ch3);
    printf("ch1=%d\n",ch1);     
    printf("ch1=%d\n",ch2);     // prints equivalent ASCII value
    printf("ch1=%d\n",ch3);
    return 0;
}
